{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.4.*",
        "teepluss/theme": "^2.0",
        "intervention/image": "^2.3",
        "nesbot/Carbon": "*",
        "anlutro/l4-settings": "^0.4.8",
        "alaouy/youtube": "dev-master",
        "anhskohbo/no-captcha": "2.*",
        "guzzlehttp/guzzle": "~5.3|~6.0",
        "laravel/socialite": "^2.0",
        "vinkla/pusher": "^2.3",
        "barryvdh/laravel-ide-helper": "^2.3",
        "barryvdh/laravel-debugbar": "^2.2",
        "barryvdh/laravel-translation-manager": "0.2.x",
        "cmgmyr/messenger": "~2.0",
        "zizaco/entrust": "5.2.x-dev",
        "cviebrock/eloquent-sluggable": "^4.0",
        "laravelcollective/html": "5.4.*",
        "laracasts/flash": "^2.0",
        "infyomlabs/laravel-generator": "dev-master",
        "infyomlabs/core-templates": "dev-master",
        "infyomlabs/swagger-generator": "dev-master",
        "jlapp/swaggervel": "dev-master",
        "vijaytupakula/transvel": "^1.0",
        "prettus/l5-repository": "^2.6",
        "fzaninotto/faker": "~1.4",
        "predis/predis": "^1.1",
        "kasp3r/link-preview": "dev-master",
        "jeremeamia/superclosure": "^2.0",
        "cartalyst/stripe-laravel": "~6.0",
        "laravelbook/ardent": "3.*",
        "nayjest/grids": "^1.3.1",
        "watson/sitemap":"^2.0",
        "doctrine/dbal": "2.5.x-dev",
        "oprudkyi/laravel-mail-logger": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*"
    },
    "autoload": {
        "files": [
            "app/Http/helpers.php"
        ],
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
